        "timeout": 5,
        "qoutes": "EURUSD,GBPUSD,USDJPY,USDCHF,AUDUSD,USDCAD,NZDUSD,EURGBP,EURJPY"        
    },
    "webserver":{
        "ip": "127.0.0.1",
